{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "strict": true,
    "baseUrl": ".",
    "paths": { "*": ["./*"] },
    "types": ["node", "vite/client"],
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src"]
}
